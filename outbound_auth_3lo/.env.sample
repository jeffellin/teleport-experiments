# OIDC discovery URL for your IdP (/.well-known/openid-configuration)
CUSTOM_IDP_DISCOVERY_URL=https://your-idp/.well-known/openid-configuration

# Allowed client IDs (comma-separated) when your JWT has client_id
CUSTOM_IDP_ALLOWED_CLIENTS=your-client-id-or-audience

# Bearer JWT used to invoke the runtime (short-lived)
CUSTOM_IDP_BEARER_TOKEN=

# Allowed audience when your JWT uses aud instead of client_id
JWT_AUDIENCE=your-audience

# Google OAuth client ID (from Google Cloud Console)
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# Google OAuth client secret (from Google Cloud Console)
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Optional: override runtime endpoint URL if needed
RUNTIME_ENDPOINT_URL=

# Optional: reverse proxy base URL for runtime invocations
PROXY_BASE_URL=https://your-proxy.example.com
